    <%= link_to event_path do %>
      <div class="fitmate-header-navbar">
        <button class='transparent-button'>
          <i class="fa-solid fa-chevron-left" style="color: #f6d0a5;"></i>
        </button>
      </div>
    <% end %>
    <h1 class="chat-title">Message</h1>
      <ul class="navbar-nav me-auto">
